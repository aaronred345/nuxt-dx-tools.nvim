{
  "name": "nuxt-dx-tools-lsp",
  "version": "1.0.0",
  "description": "Language Server Protocol implementation for Nuxt DX Tools - works with any LSP-compatible editor",
  "main": "dist/server.js",
  "bin": {
    "nuxt-dx-tools-lsp": "./dist/server.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "test": "jest",
    "prepublishOnly": "node install.js",
    "postinstall": "node install.js",
    "install:npm": "npm install && npm run build",
    "install:pnpm": "pnpm install && pnpm build",
    "install:yarn": "yarn install && yarn build",
    "install:bun": "bun install && bun run build"
  },
  "packageManager": "npm@>=9.0.0",
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt4",
    "lsp",
    "language-server",
    "language-server-protocol",
    "neovim",
    "nvim",
    "vscode",
    "vue",
    "typescript",
    "auto-imports",
    "intellisense"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronred345/nuxt-dx-tools.nvim.git",
    "directory": "lsp-server"
  },
  "bugs": {
    "url": "https://github.com/aaronred345/nuxt-dx-tools.nvim/issues"
  },
  "homepage": "https://github.com/aaronred345/nuxt-dx-tools.nvim#readme",
  "author": "aaronred345",
  "license": "MIT",
  "dependencies": {
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11",
    "vscode-uri": "^3.0.8",
    "fast-glob": "^3.3.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
